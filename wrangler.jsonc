 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"name": "tailscale-monitor",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-27",
	"triggers": { "crons": ["*/30 * * * *"] },
	"kv_namespaces": [
		{
			"binding": "TAILSCALE_STATE",
			"id": "REPLACE_WITH_KV_NAMESPACE_ID"
		}
	],
	"observability": { "enabled": false },
	"workers_dev": false,
  	"preview_urls": false
}